{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "microsoft-edge/**",
          "microsoft-edge/webview2/code/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "microsoft-edge/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/microsoft-edge/breadcrumbs/toc.json",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/edge-developer",
      "searchScope": ["Microsoft Edge", "Microsoft Edge Developer"],
      "titleSuffix": "Microsoft Edge Developer documentation",
      "uhfHeaderId": "MSDocsHeader-MSEdge"
    },
    "fileMetadata": {
    },
    "template": [
      "docs.html"
    ],
    "dest": "microsoft-edge",
    "markdownEngineName": "markdig"
  }
}
